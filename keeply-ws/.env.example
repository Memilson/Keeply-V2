# Keeply WS local config (Spring)
KEEPLY_WS_PORT=8092
KEEPLY_WS_BIND=0.0.0.0

# Auth service base URL
KEEPLY_AUTH_BASE_URL=http://localhost:18081/api/auth

# Keeply API (agent local API) for backup history
KEEPLY_AGENT_API_BASE_URL=http://localhost:25420/api/keeply

# Optional path for local session persistence
# KEEPLY_WS_SESSION_FILE=C:/path/to/.keeply-ws-session.json

# Optional client version sent to auth machine registration
# KEEPLY_WS_CLIENT_VERSION=keeply-ws/1.0.0
